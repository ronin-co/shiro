{
  "name": "shiro-codegen",
  "version": "0.0.1",
  "type": "module",
  "description": "Generates TypeScript code from RONIN models.",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ronin-co/shiro"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./zod": {
      "import": "./dist/zod.js",
      "types": "./dist/zod.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "zod": [
        "dist/zod.d.ts"
      ]
    }
  },
  "scripts": {
    "type-check": "tsc --pretty",
    "test": "bun test",
    "build": "tsup ./src/index.ts ./src/zod.ts --dts --format esm",
    "dev": "bun run build -- --watch"
  },
  "keywords": [
    "codegen",
    "ts",
    "typescript"
  ],
  "author": "ronin",
  "license": "Apache-2.0",
  "dependencies": {
    "typescript": "5.7.3"
  },
  "devDependencies": {
    "shiro-compiler": "workspace:*",
    "shiro-syntax": "workspace:*"
  }
}
