{
  "name": "shiro-syntax",
  "version": "0.2.39",
  "type": "module",
  "description": "Allows for defining RONIN queries and schemas in code.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./schema": "./dist/schema/index.js",
    "./queries": "./dist/queries/index.js",
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/schema"
      ],
      "queries": [
        "./dist/queries"
      ]
    }
  },
  "scripts": {
    "type-check": "tsc --pretty",
    "test": "bun test",
    "build": "tsup ./src/index.ts ./src/schema/index.ts ./src/queries/index.ts --dts --format esm",
    "dev": "bun run build -- --watch"
  },
  "keywords": [
    "database",
    "schema",
    "primitives"
  ],
  "author": "ronin",
  "license": "Apache-2.0",
  "devDependencies": {
    "expect-type": "1.2.0",
    "shiro-compiler": "workspace:*"
  }
}
