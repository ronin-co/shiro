    "@ronin/engine": "0.1.23",
    "chalk-template": "1.1.0",
    "get-port": "7.1.0",
    "ini": "5.0.0",
    "json5": "2.2.3",
    "open": "10.1.0",
    "ora": "8.1.1",
    "resolve-from": "5.0.0"
  },
  "devDependencies": {
    "@types/ini": "4.1.1",
    "bun-bagel": "1.1.0",
