{
  "name": "shiro-cli",
  "version": "0.3.13",
  "type": "module",
  "description": "The command-line interface for RONIN.",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "shiro": "./dist/bin/index.js"
  },
